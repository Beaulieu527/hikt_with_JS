<%= current_user.email%>
<div class="container">
    <div class="row">
            <%@users.each do |user|%>
            <div class="col-lg-4 col-sm-6 mb-4">
                <div class="card h-100">
                    <div class="card-body">
                        <h5 class="card-title"><%=user.email%></h5>
                    </div>
                    <a class="btn btn-info" href="/users/<%=user.id%>">Show user</a>
                </div>
            </div>
            <%end%>
    </div>
</div>